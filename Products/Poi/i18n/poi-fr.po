#. Default: "Add response"
#: ./skins/Poi/poi_issue_view.pt
msgid "Add response"
msgstr "Ajouter une réponse"

#. Default: "Cancel"
#: ./skins/Poi/poi_response_delete_confirm.cpt
msgid "Cancel"
msgstr "Annuler"

#. Default: "Delete"
